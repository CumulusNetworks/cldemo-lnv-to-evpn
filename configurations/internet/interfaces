#########
## Configured by Ansible
#########

# Loopback interface
auto lo
iface lo inet loopback
  address 10.0.0.253/32
  address 4.2.2.2/32
  alias loopback interface

# Management interface
auto eth0
iface eth0
    address 192.168.0.253/24

# Port to exit01
auto swp1
iface swp1
  mtu 9216
  alias to Leaf01

# Port to exit02
auto swp2
iface swp2
  mtu 9216
  alias to Leaf02

#
auto swp48
iface swp48
  mtu 9216
